{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Database": "",
    "Cache": ""
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 30
      },
      {
        "Endpoint": "POST:/shorten",
        "Period": "1m",
        "Limit": 5
      }
    ]
  },
  "RunMigrations": false
}
